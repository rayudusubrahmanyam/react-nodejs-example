name: learn-github-actions
on: 
  push: [develop]
jobs:
  check-cat-version:
    runs-on: ubuntu-latest
    steps:
      - name: checkout
        uses: actions/checkcout@v3
      - name: Node Setup
        uses: actions/nodesetup@v3
        with:
          node-version: '14'
      - run: npm install -g bats
      - run: bats -v         